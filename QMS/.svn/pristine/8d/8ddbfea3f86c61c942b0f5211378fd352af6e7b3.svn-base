import httpService from '@/libs/service'

export default {
    
    add: function (data) {
        return httpService.post('/hr/sysprofession/add', data)
    },
    list: function (data) {
        return httpService.get('/hr/sysprofession/list', data)
    },
    delete: function (data) {
        return httpService.post('/hr/sysprofession/delete', data)
    },
    update:function (data) {
        return httpService.post('/hr/sysprofession/edit', data)
    },
}