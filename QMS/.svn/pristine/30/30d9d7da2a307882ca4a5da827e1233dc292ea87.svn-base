import httpService from '@/libs/service'

export default {
    gettree: function (data) {
        return httpService.get('/role/gettree', data)
    },
    getlist: function (data) {
        return httpService.get('/role/getlist', data)
    },
    add: function (data) {
        return httpService.post('/role/add', data)
    },
    edit: function (data) {
        return httpService.post('/role/edit', data)
    },
    delete: function (data) {
        return httpService.post('/role/delete', data)
    }
}