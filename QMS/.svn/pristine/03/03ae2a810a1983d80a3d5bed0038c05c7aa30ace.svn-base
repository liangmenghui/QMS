import httpService from '@/libs/service'

export default {
    getlist: function (data) {
        return httpService.get('/pmp/config/getlist', data)
    },
    getdetail: function (data) {
        return httpService.get('/pmp/config/getdetail', data)
    },
    save: function (data) {
        return httpService.post('pmp/config/save', data)
    }
}