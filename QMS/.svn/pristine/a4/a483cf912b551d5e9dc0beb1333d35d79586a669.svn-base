import httpService from '@/libs/service'

export default {
    getlist: function (data) {
        return httpService.get('/perm/getlist', data)
    },
    add: function (data) {
        return httpService.post('/perm/add', data)
    },
    edit: function (data) {
        return httpService.post('/perm/edit', data)
    },
    delete: function (data) {
        return httpService.post('/perm/delete', data)
    }
}