import httpService from '@/libs/service'

export default {
    getlist: function (data) {
        return httpService.get('/approvedItems/getlist', data)
    },
    add: function (data) {
        return httpService.post('/approvedItems/add', data)
    },
    edit: function (data) {
        return httpService.post('/approvedItems/edit', data)
    },
    delete: function (data) {
        return httpService.post('/approvedItems/delete', data)
    },
    getrecords: function (data) {
        return httpService.get('/approvedItemsRecord/getlist',data)
    }
}